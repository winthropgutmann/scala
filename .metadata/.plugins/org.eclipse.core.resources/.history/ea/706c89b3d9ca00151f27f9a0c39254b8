import scala.io._
import java.io._
import java.nio._
import scala.util.matching.Regex

object tst {
  def main(args: Array[String]): Unit = {
      val line = "v49rc"
      val pattern = new Regex("[0-9]");
        val version = (pattern findAllIn line).mkString
        if(version.toInt >= 47){
          println("more than 47")
        }else{
          println(version)
        }

  }
} 