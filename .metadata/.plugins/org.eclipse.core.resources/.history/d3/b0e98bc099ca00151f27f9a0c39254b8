import scala.io.Source

object tst {
    def main(args: Array[String]): Unit = {
      val filename = "LocalSettings.rb"
      for (line <- Source.fromFile(filename).getLines()){
        if(line.contains("provisioning_client")){
          
          var myVar = line.split(" ")
          println(myVar(0))
        }
      }
  }
}