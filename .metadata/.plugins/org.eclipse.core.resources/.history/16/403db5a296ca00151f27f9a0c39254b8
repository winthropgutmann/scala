import scala.io.Source

object tst {
    def main(args: Array[String]): Unit = {
//      val filename = "C:\\Users\Winthrop\\scalaprojects\\helloworld\\src\\LocalSettings.rb"
      val filename = "myFile.txt"
      for (line <- Source.fromFile(filename).getLines()){
        println(line)
      }
  }
}